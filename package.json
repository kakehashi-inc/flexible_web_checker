{
  "name": "flexible_web_checker",
  "version": "1.0.0",
  "description": "flexible web checker",
  "scripts": {
    "build:css": "npx @tailwindcss/cli -i frontend/tailwindcss/input.css -o static/css/theme.css",
    "build:css:prod": "NODE_ENV=production npx @tailwindcss/cli -i frontend/tailwindcss/input.css -o static/css/theme.css --minify",
    "watch:css": "npx @tailwindcss/cli -i frontend/tailwindcss/input.css -o static/css/theme.css --watch",
    "build:ts": "gulp build-ts",
    "build:js": "gulp build-js",
    "build:scss": "gulp build-scss",
    "format:ts": "prettier --write \"frontend/ts/**/*.ts\"",
    "format:js": "prettier --write \"frontend/js/**/*.js\"",
    "format:style": "prettier --write \"frontend/scss/**/*.{css,scss}\"",
    "lint": "tsc --noEmit -p tsconfig.lint.json",
    "setup:libs": "gulp setup-libs",
    "setup:fa": "gulp setup-fa",
    "icons": "gulp icons"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sweetalert2": "^11.26.3",
    "tailwindcss": "^4.1.16"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@tailwindcss/cli": "^4.1.16",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/styled-jsx": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.1.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-standard": "^5.0.0",
    "favicons": "^6.2.2",
    "glob": "^11.0.3",
    "gulp": "^4.0.2",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^10.0.0",
    "gulp-rename": "^2.1.0",
    "gulp-sass": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.16.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-eslint": "^16.4.2",
    "prettier-eslint-cli": "^8.0.1",
    "sass": "1.69.7",
    "styled-jsx": "^5.1.7",
    "typescript": "^5.9.3"
  }
}
